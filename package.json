    "eslint-plugin-prettier": "^3.4.1",
    "execa": "^7.1.1",
    "globby": "11.0.3",
    "husky": "^6.0.0",
    "inquirer": "^8.2.1",
    "js-yaml": "^4.1.0",
    "lint-staged": "^13.2.3",
    "next-contentlayer": "^0.3.4",
    "postcss": "^8.4.12",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.3",
