{
  "name": "tailwind-nextjs-typescript-starter-blog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "next dev",
    "dev": "next dev",
    "prebuild": "contentlayer build",
    "build": "next build",
    "postbuild": "npm run sitemap && npm run rss",
    "sitemap": "tsx ./scripts/generate-sitemap.ts",
    "rss": "tsx ./scripts/generate-rss.ts",
    "compose": "tsx ./scripts/compose.ts",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx .",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "prettier": "prettier --check --write --no-plugin-search \"**/*.(js|jsx|tsx|ts|css|less|scss|sass|md|yaml)\"",
    "prepare": "husky install",
    "lint-staged:lint": "eslint"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:lint",
    "**/*.{js,jsx,tsx,ts,css,less,scss,sass,md,yaml}": [
      "prettier --write --no-plugin-search"
    ]
  },
  "pnpm": {
    "overrides": {
      "@opentelemetry/api": "1.4.1",
      "@opentelemetry/core": "1.13.0",
      "@opentelemetry/exporter-trace-otlp-grpc": "0.39.1",
      "@opentelemetry/resources": "1.13.0",
      "@opentelemetry/sdk-trace-base": "1.13.0",
      "@opentelemetry/sdk-trace-node": "1.13.0",
      "@opentelemetry/semantic-conventions": "1.13.0"
    }
  },
  "dependencies": {
    "@analytics/google-tag-manager": "^0.5.5",
