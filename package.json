{
  "name": "tailwind-nextjs-typescript-starter-blog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "next dev",
    "dev": "next dev",
    "build": "next build",
    "postbuild": "npm run sitemap && npm run rss",
    "sitemap": "tsx ./scripts/generate-sitemap.ts",
    "rss": "tsx ./scripts/generate-rss.ts",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx .",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "prettier": "prettier --check --write --no-plugin-search \"**/*.(js|jsx|tsx|ts|css|less|scss|sass|md|yaml)\"",
    "prepare": "husky install",
    "lint-staged:lint": "eslint"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:lint",
    "**/*.{js,jsx,tsx,ts,css,less,scss,sass,md,yaml}": [
      "prettier --write --no-plugin-search"
    ]
  },
  "pnpm": {
    "overrides": {
      "@opentelemetry/api": "1.4.1",
      "@opentelemetry/core": "1.13.0",
      "@opentelemetry/exporter-trace-otlp-grpc": "0.39.1",
      "@opentelemetry/resources": "1.13.0",
      "@opentelemetry/sdk-trace-base": "1.13.0",
      "@opentelemetry/sdk-trace-node": "1.13.0",
      "@opentelemetry/semantic-conventions": "1.13.0"
    }
  },
  "dependencies": {
    "@fontsource/inter": "4.5.2",
    "@mailchimp/mailchimp_marketing": "^3.0.58",
    "fs-extra": "^11.1.1",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.2",
    "image-size": "1.0.0",
    "lodash-es": "^4.17.21",
    "mdx-bundler": "^9.2.1",
    "next": "^13.4.10",
    "next-seo": "^6.1.0",
    "next-themes": "^0.2.1",
    "preact": "^10.6.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reading-time": "1.3.0",
    "rehype-autolink-headings": "^6.1.0",
    "rehype-citation": "^0.2.0",
    "rehype-katex": "^6.0.2",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "^1.1.3",
    "rehype-slug": "^5.0.0",
    "remark": "^14.0.3",
    "remark-footnotes": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "sharp": "^0.28.3",
    "unist-util-visit": "^4.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.3",
    "@antfu/eslint-config-react": "^0.35.3",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@commitlint/types": "^17.4.4",
    "@jannajs/lint": "^2.3.5",
    "@next/bundle-analyzer": "12.0.9",
    "@prettier/plugin-xml": "^3.1.1",
    "@svgr/webpack": "^6.1.2",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/dedent": "^0.7.0",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^11.0.1",
    "@types/github-slugger": "^2.0.0",
    "@types/inquirer": "^8.2.0",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash-es": "^4.17.8",
    "@types/prettier": "^2.4.4",
    "@types/react": "^17.0.41",
    "@typescript-eslint/parser": "^5.15.0",
    "autoprefixer": "^10.4.4",
    "chalk": "^5.0.1",
    "chokidar": "^3.5.3",
    "commander": "^9.1.0",
    "cross-env": "^7.0.3",
    "dedent": "^0.7.0",
    "esbuild": "^0.18.13",
    "eslint": "^8.45.0",
    "eslint-config-next": "13.4.8",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^3.3.1",
    "execa": "^7.1.1",
    "express": "^4.17.3",
    "file-loader": "^6.0.0",
    "globby": "11.0.3",
    "husky": "^6.0.0",
    "inquirer": "^8.1.1",
    "js-yaml": "^4.1.0",
    "lint-staged": "^13.2.3",
    "mdast-util-to-string": "^3.1.0",
    "next-contentlayer": "^0.3.4",
    "postcss": "^8.4.12",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.1.4",
    "socket.io": "^4.4.0",
    "socket.io-client": "^4.4.0",
    "tailwindcss": "^3.0.23",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "vfile": "^5.3.2"
  }
}
